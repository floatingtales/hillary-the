---
import { Image } from "astro:assets";
import TitleHorizontalLine from "../shared/TitleHorizontalLine.astro";

import PsychVisionResult from "@assets/project-assets/psych-vision/psychvision-result.png";
---

<section
  id="final-result"
  class="flex w-full flex-col items-center justify-center gap-16"
>
  <TitleHorizontalLine title="FINAL RESULT" />
  <div class="flex grow items-center justify-center">
    <Image
      src={PsychVisionResult}
      alt="Psych Vision Result"
      width={PsychVisionResult.width / 3}
      class="grow"
    />
  </div>
</section>
